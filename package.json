{
  "name": "vue-calculator",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "dev": "NODE_ENV=test start-test serve 8080 cy:open",
    "ci": "NODE_ENV=test start-test serve 8080 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "serve-static": "^1.13.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.8.1",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "4.9.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "start-server-and-test": "1.11.0",
    "vue-template-compiler": "^2.5.21"
  },
  "engines": {
    "node": "10.13.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "description": "Super simple calculator made with Vue.js",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylbutlr/vue-calculator.git"
  },
  "author": "Kyle Butler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylbutlr/vue-calculator/issues"
  },
  "homepage": "https://github.com/kylbutlr/vue-calculator#readme"
}
